#:import md_icons kivymd.icon_definitions.md_icons
ScreenManager:
    id: manager
    MDScreen:
        name: "inicio"

        # backgroud image

        # title
        MDLabel:
            halign: "center"
            pos_hint: {'center_y': .75}
            text: f"[font=assets/Fonts/Montserrat-Regular]Brazil/Ecuador Tour[/font]"
            markup: True
            font_size: 32


        # start button
        MDFillRoundFlatButton:
            pos_hint: {'center_x': .5, 'center_y': .2}
            text: f"[font=assets/Fonts/Montserrat-Regular]Começar Tour[/font]"
            size_hint_x: 0.8
            on_release: manager.current = "idioma"

        MDLabel:
            halign: "center"
            pos_hint: {'center_y': .128}
            text: f"[font=assets/Fonts/Montserrat-Regular]Modo Escuro[/font]"
            markup: True
            font_size: 10
        MDSwitch:
            pos_hint: {'center_x': .5, 'center_y': .10}
            on_active: app.check(*args)
            size: "40dp", "30dp"

    # language choose
    MDScreen:
        name: "idioma"
        MDToolbar:
            id: toolbar
            title: f"[font=assets/Fonts/Montserrat-Regular]Selecione um idioma:[/font]"
            pos_hint: {"top": 1}
            anchor_title: "center"
        MDIconButton:
            icon:"arrow-left"
            pos_hint: {"center_y": .95}
            on_release: manager.current = "inicio"
        MDSwiper:
            size_hint_y: None
            height: root.height - toolbar.height - dp(40)
            y: root.height - self.height - toolbar.height - dp(20)
            items_spacing:  "8dp"

            MDSwiperItem
                RelativeLayout:

                    FitImage:
                        source: "assets/images/brazil.png"
                        radius: [20,]

                    MDBoxLayout:
                        adaptive_height: True
                        spacing: "12dp"

                        MDFillRoundFlatButton:
                            user_font_size: "30sp"
                            opposite_colors: True
                            text: "Português Brasileiro"
                            size_hint_x: 1
                            on_release: manager.current = "pais-pt"
            MDSwiperItem
                RelativeLayout:

                    FitImage:
                        source: "assets/images/equador.png"
                        radius: [20,]

                    MDBoxLayout:
                        adaptive_height: True
                        spacing: "12dp"

                        MDFillRoundFlatButton:
                            id: icon
                            icon: "earth"
                            user_font_size: "30sp"
                            opposite_colors: True
                            text: "Español"
                            size_hint_x: 1
                            on_release: manager.current = "pais-es"

    # country choose pt
