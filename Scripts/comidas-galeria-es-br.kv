                            font_name: "assets/Fonts/Poppins-Regular"
                            on_release: manager.current="Beijinho"
                        SmartTileWithLabel:
                            text: "Bolinho de chuva(bollo de lluvia)"
                            font_name: "assets/Fonts/Poppins-Regular"
