MDScreen:
    name:"Azuay es-eq"
    MDFloatLayout:
        FitImage:
            id: bg_image
        MDToolbar:
            id: toolbar
            text: "df"
            pos_hint: {"top": 1}
            md_bg_color: [0,0,1,0]
            anchor_title: "center"
        MDIconButton:
            icon:"arrow-left"
            pos_hint: {"center_y": .95}
            on_release: manager.current = "comidas"
        MDCard:
            orientation: "vertical"
            size_hint: .94, None
            height: box_top.height + box_bottom.height
            focus_behavior: True
            ripple_behavior: True
            pos_hint: {"center_x": .5, "center_y": .19}
            radius: 15, 15, 15, 15
            elevation: 10

