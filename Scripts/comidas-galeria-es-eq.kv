#galeria-comidas-es-eq
        MDScreen: 
            name: "comidas-es-eq"
            MDFloatLayout:
                orientation: "vertical"
                MDIconButton:
                    icon:"arrow-left"
                    pos_hint: {"center_y": .95}
                    on_release: manager.current = "es-eq"
                MDLabel:
                    id: title
                    text: "Cocina"
                    pos_hint: {"center_y": .95}
                    halign:"center"
                    font_name:"assets/Fonts/Poppins-Regular"
                    font_size: "20sp"
                ScrollView:
                    do_scroll_y: True
                    do_scroll_x: False
                    pos_hint:{"center_x": .5, "y": 0}
                    size_hint_y: .9
                    bar_width: 0
                    MDGridLayout:
                        cols: 2
                        row_default_height: (self.width - self.cols*self.spacing[0]) / self.cols
                        row_force_default: True
                        adaptive_height: True
                        padding: dp(4), dp(4)
                        spacing: dp(4)

                        SmartTileWithLabel:
                            text: "Ceviche"
                            font_name: "assets/Fonts/Poppins-Regular"
                            on_release: manager.current="Ceviche"
                        SmartTileWithLabel:
                            text: "Encocado de Pescado"
                            font_name: "assets/Fonts/Poppins-Regular"
                            on_release: manager.current="Encocado de Pescado"
                        SmartTileWithLabel:
                            text: "Cazuela de Mariscos"
                            font_name: "assets/Fonts/Poppins-Regular"
                            on_release: manager.current="Cazuela de Mariscos"
                        SmartTileWithLabel:
                            text: "Cocadas Blancas y Cocadas Negras"
                            font_name: "assets/Fonts/Poppins-Regular"
                            on_release: manager.current="Cocadas Blancas y Cocadas Negras"
                        SmartTileWithLabel:
